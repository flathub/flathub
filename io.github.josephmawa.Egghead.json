    "*.la",
    "*.a"
  ],
  "modules": [
    {
      "name": "egghead",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "https://github.com/josephmawa/Egghead",
          "tag": "v0.1.0",
          "commit": "1b4b843c54b493fd4a3bfa82d8bc34ee3f375b1f"
        }
      ]
    }
  ]
}
