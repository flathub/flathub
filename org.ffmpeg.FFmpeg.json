{
   "name":"ffmpeg",
   "cleanup":[
      "/include",
      "/lib/pkgconfig",
      "/share"
   ],
   "build-options":{
      "arch":{
         "i386":{
            "config-opts":[
               "--arch=x86"
            ]
         },
         "arm":{
            "config-opts":[
               "--arch=arm"
            ]
         }
      }
   },
   "config-opts":[
      "--disable-all",
      "--enable-shared",
      "--disable-static",
      "--enable-swscale",
      "--enable-gnutls",
      "--enable-avfilter",
      "--enable-avcodec",
      "--enable-avformat",
      "--enable-decoder=aac"
   ],
   "sources":[
      {
         "type":"archive",
         "url":"https://www.ffmpeg.org/releases/ffmpeg-3.3.4.tar.gz",
         "sha256":"bd0d1adf962c217c29c622314f339b085ae126fa36d88ad805a8fc490a86c601"
      }
   ]
}
