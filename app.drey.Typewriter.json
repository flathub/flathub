{
    "id" : "app.drey.Typewriter",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "48",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "typewriter",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin",
        "env" : {
            "RUST_BACKTRACE" : "1",
            "RUSTFLAGS" : "-O",
            "PROFILE" : "release"
        }
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "blueprint-compiler",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://gitlab.gnome.org/jwestman/blueprint-compiler",
                    "tag" : "v0.16.0"
                }
            ]
        },
        {
            "name" : "libpanel",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://gitlab.gnome.org/GNOME/libpanel.git",
                    "tag" : "1.8.0"
                }
            ]
        },
        {
            "name" : "typewriter",
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://gitlab.gnome.org/api/v4/projects/32310/packages/generic/typewriter/0.1.0/typewriter-0.1.0.tar.xz",
                    "sha256": "54a50c943c78d4f303d7a8597db86c2a25c6349d8e932e8afd8b5cc361a4596d"
                }
            ]
        }
    ]
}
