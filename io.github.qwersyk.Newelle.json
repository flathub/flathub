{
    "app-id" : "io.github.qwersyk.Newelle",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "44",
    "sdk" : "org.gnome.Sdk",
    "command" : "newelle",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--talk-name=org.freedesktop.Flatpak",
        "--filesystem=home"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "newelle",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://github.com/qwersyk/Newelle.git",
                    "tag": "0.1.3.2",
                    "commit": "98170f3cc38890d4c50e5ac6630a984d3ca8e5b6"
                }
            ]
        }
    ]
}
