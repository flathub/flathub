<component type="console-application">
    <id>dev.paullee.scraterpreter.Scrapec</id>

    <name>Scraterpreterc</name>
    <summary>Scraterpreterc is a compiler for Scraterpreter.</summary>

    <metadata_license>MIT</metadata_license>
    <project_license>MIT</project_license>

    <description>
        <p>
            Scraterpreterc compiles Scratch projects (.sb3) into .scrape files. Use Scraterpreterc to compile .sb3 (Scratch Project) files to .scrape files, then use Scraterpreter to run .scrape files.
        </p>
        <p>
            For more information, visit https://github.com/scraterpreter/scrapec.
        </p>
    </description>

    <categories>
        <category>Utility</category>
        <category>Construction</category>
    </categories>

    <provides>
        <binary>scraterpreterc</binary>
    </provides>

    <url type="homepage">https://github.com/scraterpreter/scrapec</url>
    <url type="help">https://discord.gg/3zV37x4</url>

    <developer_name>Scraterpreter Authors</developer_name>
    <update_contact>contact_AT_paullee.dev</update_contact>

    <screenshots>
