[Application]
name=io.github.HaruNashii.RustInfoInvest
runtime=org.freedesktop.Platform/x86_64/25.08
sdk=org.freedesktop.Sdk/x86_64/25.08
command=RustInfoInvest

[Context]
shared=network;ipc;
sockets=x11;wayland;pulseaudio;
devices=dri;
filesystems=~/.local/share/icons;

[Extension io.github.HaruNashii.RustInfoInvest.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=io.github.HaruNashii.RustInfoInvest.Debug;