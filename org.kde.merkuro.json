{
    "id": "org.kde.merkuro",
    "runtime": "org.kde.Platform",
    "runtime-version": "6.10",
    "base": "io.qt.qtwebengine.BaseApp",
    "base-version": "6.10",
    "sdk": "org.kde.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "run_merkuro.sh",
    "finish-args": [
        "--device=dri",
        "--env=SASL_PATH=/app/lib/sasl2",
        "--filesystem=xdg-run/gnupg:ro",
        "--filesystem=~/.gnupg:create",
        "--own-name=org.freedesktop.Akonadi",
        "--own-name=org.freedesktop.Akonadi.*",
        "--own-name=org.kde.accountwizard",
        "--own-name=org.kde.akonadiconsole.*",
        "--own-name=org.kde.kalendarac",
        "--own-name=org.kde.merkuro",
        "--own-name=org.kde.pim.*",
        "--own-name=org.kde.pimsettingexporter",
        "--own-name=org.kde.sieveeditor",
        "--share=ipc",
        "--share=network",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--system-talk-name=org.freedesktop.NetworkManager",
        "--talk-name=org.kde.Solid.PowerManagement",
        "--talk-name=org.kde.StatusNotifierWatcher",
        "--talk-name=org.kde.kded6",
        "--talk-name=org.kde.kglobalaccel",
        "--talk-name=org.kde.knotify",
        "--talk-name=org.kde.kwalletd6"
    ],
    "cleanup": [
        "/lib/cmake",
        "/lib/pkgconfig",
        "/lib/plugins/designer",
        "/include",
        "/docs",
        "/mkspecs",
        "/share/man",
        "/bin/afmtodit",
        "/usr/bin/xsd",
        "/lib/libKF6*.so",
        "/lib/libboost_*.so",
        "*.a",
        "*.la"
    ],
    "copy-icon": true,
    "modules": [
        {
            "name": "libetebase",
            "buildsystem": "simple",
            "build-commands": [
                "mkdir .cargo",
                "cp cargo/config .cargo/",
                "source /usr/lib/sdk/rust-stable/enable.sh && cargo --offline fetch --manifest-path Cargo.toml",
                "source /usr/lib/sdk/rust-stable/enable.sh && cargo --offline build --release",
                "PREFIX=/app make V=1 VERBOSE=1 pkgconfig",
                "PREFIX=/app make V=1 VERBOSE=1 install",
                "mv /app/lib/libetebase.so /app/lib/libetebase.so.0",
                "ln -s /app/lib/libetebase.so.0 /app/lib/libetebase.so"
            ],
            "build-options": {
                "append-path": "/usr/lib/sdk/rust-stable/bin"
            },
            "sources": [
                "libetebase-dependencies-cargo-generator.json",
                {
                    "type": "archive",
                    "url": "https://github.com/etesync/libetebase/archive/v0.5.8/libetebase-0.5.8.tar.gz",
                    "sha256": "4f2623a6e194aa3df69307d13ad00c42ec853a1af02ca329e35cd829ff472145",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 235050,
                        "stable-only": true,
                        "url-template": "https://github.com/etesync/libetebase/archive/v$version/libetebase-$version.tar.gz"
                    }
                }
            ]
        },
        {
            "name": "cyrus-sasl2",
            "config-opts": [
                "--with-dblib=berkeley",
                "--without-pam",
                "--without-opie",
                "--without-des",
                "--disable-gssapi",
                "--enable-cram",
                "--enable-digest",
                "--enable-otp",
                "--enable-plain",
                "--enable-login",
                "--with-plugindir=/app/lib/sasl2"
            ],
            "build-options": {
                "cflags": "-Wno-implicit-function-declaration -Wno-incompatible-pointer-types"
            },
            "no-parallel-make": true,
            "sources": [
                {
                    "type": "archive",
                    "url": "https://github.com/cyrusimap/cyrus-sasl/archive/cyrus-sasl-2.1.28.tar.gz",
                    "sha256": "3e38933a30b9ce183a5488b4f6a5937a702549cde0d3287903d80968ad4ec341",
