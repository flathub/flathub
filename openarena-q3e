CVARS="+set fs_basegame baseoa +set protocol 71 +set com_protocol 72 +set cl_voip 1"

archs=`uname -m`
case "$archs" in
    i?86) archs="i386" ;;
    x86_64) archs="x64" ;;
    ppc64) archs="ppc64" ;;
    aarch64) archs="aarch64" ;;
esac

for arch in $archs; do
  #  test -x ./quake3e.$arch || continue
    exec ./quake3e.$arch $CVARS "$@"
done


