{
    "id" : "io.github.Ethanscharlie.albumripper",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "48",
    "sdk" : "org.gnome.Sdk",
    "command" : "albumripper",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        "python3-requests.json",
        {
            "name" : "albumripper",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [{
                "type" : "git",
                "url" : "https://github.com/Ethanscharlie/albumripper.git",
                "tag": "v1.0.1"
            }]
        }
    ]
}
