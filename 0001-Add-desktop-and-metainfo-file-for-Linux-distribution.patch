From 3164b766a38ac70ca4dfff61127bf67661775d19 Mon Sep 17 00:00:00 2001
From: Bastien Nocera <hadess@hadess.net>
Date: Fri, 11 Oct 2024 10:58:04 +0200
Subject: [PATCH 1/3] Add data files for Linux distributions

This adds a desktop file, a metainfo file and a mime-type file that
are used by Linux software installation front-ends to present
applications, and add desktop integration.

This will be used by the Flatpak package being put together for
distribution on Flathub:
https://github.com/flathub/flathub/pull/5701
---
 .../io.github.labbots.NiimPrintX.desktop      |  10 +++++
 .../io.github.labbots.NiimPrintX.metainfo.xml |  36 ++++++++++++++++++
 .../io.github.labbots.NiimPrintX.mime.xml     |   7 ++++
 assets/linux/main-window.png                  | Bin 0 -> 123429 bytes
 4 files changed, 53 insertions(+)
 create mode 100755 assets/linux/io.github.labbots.NiimPrintX.desktop
 create mode 100755 assets/linux/io.github.labbots.NiimPrintX.metainfo.xml
 create mode 100644 assets/linux/io.github.labbots.NiimPrintX.mime.xml
 create mode 100644 assets/linux/main-window.png

diff --git a/assets/linux/io.github.labbots.NiimPrintX.desktop b/assets/linux/io.github.labbots.NiimPrintX.desktop
new file mode 100755
index 000000000000..515199e81603
--- /dev/null
+++ b/assets/linux/io.github.labbots.NiimPrintX.desktop
@@ -0,0 +1,10 @@
+[Desktop Entry]
+Name=NiimPrintX
+Comment=Create and print labels on Niimbot printers
+Keywords=label;card;print;office;barcode;bluetooth;
+Exec=python -m NiimPrintX.ui %U
+Icon=io.github.labbots.NiimPrintX
+Terminal=false
+Type=Application
+Categories=Office;
+StartupNotify=true
diff --git a/assets/linux/io.github.labbots.NiimPrintX.metainfo.xml b/assets/linux/io.github.labbots.NiimPrintX.metainfo.xml
new file mode 100755
index 000000000000..50ab22b04d80
--- /dev/null
+++ b/assets/linux/io.github.labbots.NiimPrintX.metainfo.xml
@@ -0,0 +1,36 @@
+<?xml version="1.0" encoding="UTF-8"?>
+<component type="desktop">
+    <id>io.github.labbots.NiimPrintX</id>
+    <launchable type="desktop-id">io.github.labbots.NiimPrintX.desktop</launchable>
+    <provides>
+        <id>io.github.labbots.NiimPrintX.desktop</id>
+    </provides>
+    <name>NiimPrintX</name>
+    <summary>Create and print labels on Niimbot printers</summary>
+    <developer_name>labbots</developer_name>
+    <url type="homepage">https://github.com/labbots/NiimPrintX/</url>
+    <url type="bugtracker">https://github.com/labbots/NiimPrintX/issues/</url>
+    <metadata_license>0BSD</metadata_license>
+    <project_license>GPL-3.0</project_license>
+    <content_rating type="oars-1.1" />
+    <recommends>
+        <control>keyboard</control>
+        <control>pointing</control>
+    </recommends>
+    <screenshots>
+        <screenshot type="default">
+            <image>https://raw.githubusercontent.com/labbots/NiimPrintX/refs/heads/main/assets/linux/main-window.png</image>
+            <caption>Main window</caption>
+        </screenshot>
+    </screenshots>
+    <description>
+      <p>
+        NiimPrintX is an application to design and print labels using NiimBot label printers via Bluetooth.
+      </p>
+    </description>
+    <releases>
+        <release version="0.0.47" date="2024-07-01">
+            <url>https://github.com/labbots/NiimPrintX/releases/tag/v0.0.47</url>
+        </release>
+    </releases>
+</component>
diff --git a/assets/linux/io.github.labbots.NiimPrintX.mime.xml b/assets/linux/io.github.labbots.NiimPrintX.mime.xml
new file mode 100644
index 000000000000..f188fe948788
--- /dev/null
+++ b/assets/linux/io.github.labbots.NiimPrintX.mime.xml
@@ -0,0 +1,7 @@
+<?xml version="1.0" encoding="UTF-8"?>
+<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
+  <mime-type type="application/x-niim">
+    <comment>NiimPrintX label</comment>
+    <glob pattern="*.niim"/>
+  </mime-type>
+</mime-info>
