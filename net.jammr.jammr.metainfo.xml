<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
    <id>net.jammr.jammr</id>
    <metadata_license>MIT</metadata_license>
    <name>jammr client</name>
    <summary>Play with musicians over the internet</summary>
    <description>
        <p>
            jammr lets you jam together online. It is possible to play jam
            sessions with musicians around the world over the internet. Any
            instrument or microphone that can be recorded on your computer can
            be used to play.
        </p>
    </description>
