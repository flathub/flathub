{
    "id" : "io.github.gaheldev.Millisecond",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "49",
    "sdk" : "org.gnome.Sdk",
    "command" : "millisecond",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--filesystem=/boot:ro",
        "--filesystem=/sys:ro",
        "--device=all"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "millisecond",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "http://github.com/gaheldev/Millisecond.git",
                    "tag" : "v0.2.0"
                }
            ]
        }
    ]
}
