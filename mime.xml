		<icon name="eagle"/>
		<glob-deleteall/>
		<sub-class-of type="text/xml"/>
		<glob pattern="*.epf"/>
	</mime-type>

	<mime-type type="application/x-eagle-library">
		<comment>EAGLE Library</comment>
		<comment xml:lang="de">EAGLE Bibliothek</comment>
		<icon name="eagle"/>
		<glob-deleteall/>
		<sub-class-of type="text/xml"/>
		<glob pattern="*.lbr"/>
	</mime-type>
</mime-info>
