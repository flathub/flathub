{
    "id" : "dev.louiscouture.path",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "49",
    "sdk" : "org.gnome.Sdk",
    "command" : "paths3",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--filesystem=~/.bashrc.d:create",
        "--filesystem=~/.bashrc"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "paths",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://github.com/Kibranoz/Paths.git",
                    "tag": "1.0.1",
                    "commit": "74bb75e11e6a1c63e02149f0ae84023d411b96d2"
                }
            ]
        }
    ]
}
