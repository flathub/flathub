 		set(INSTALL_DATA_DIR ".")
 		set(INSTALL_ICON_DIR ".")
 		set(INSTALL_APPLICATION_DIR "unused")
+		set(INSTALL_METAINFO_DIR "unused")
 		set(INSTALL_BIN_DIR ".")
 	elseif(APPLE)
 		set(INSTALL_DATA_DIR ".")
 		set(INSTALL_ICON_DIR "unused")
 		set(INSTALL_APPLICATION_DIR "unused")
+		set(INSTALL_METAINFO_DIR "unused")
 		set(INSTALL_BIN_DIR ".")
