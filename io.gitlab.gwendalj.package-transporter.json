{
    "app-id" : "io.gitlab.gwendalj.package-transporter",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "44",
    "sdk" : "org.gnome.Sdk",
    "command" : "package-transporter",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--filesystem=/var/lib/flatpak/app:ro",
        "--filesystem=~/.var/app/:ro"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "package-transporter",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://gitlab.com/GwendalJ/package-transporter",
                    "commit": "c9efa06518ca2dd679fff3834dc141d9bc0a0544",
                    "tag": "0.1.0"
                }
            ]
        }
    ]
}

