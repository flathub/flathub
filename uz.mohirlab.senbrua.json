{
    "app-id": "uz.mohirlab.senbrua",
    "runtime": "org.gnome.Platform",
    "runtime-version": "48",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [],
    "command": "senbrua",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--socket=pulseaudio",
        "--device=dri",
        "--filesystem=xdg-music:rw",
        "--filesystem=xdg-download:rw"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "build-options": {
    },
    "modules": [
        {
            "name": "senbrua",
            "buildsystem": "meson",
            "config-opts": [
                "-Dskip_ts=true"
            ],
            "sources": [
                {
                    "type": "git",
                    "url": "https://github.com/otabekoff/senbrua.git",
                    "tag": "v1.0.0"
                }
            ]
        }
    ]
}
