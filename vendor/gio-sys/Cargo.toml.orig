[build-dependencies]
system-deps = "7"

[dependencies]
libc = "0.2"

[dependencies.glib-sys]
workspace = true

[dependencies.gobject-sys]
workspace = true

[target."cfg(windows)".dependencies.windows-sys]
version = ">=0.52, <=0.61"
features = ["Win32_Networking_WinSock"]

[dev-dependencies]
shell-words = "1.0.0"
tempfile = "3"

[features]
v2_58 = []
v2_60 = ["v2_58"]
v2_62 = ["v2_60"]
v2_64 = ["v2_62"]
v2_66 = ["v2_64"]
v2_68 = ["v2_66"]
v2_70 = ["v2_68"]
v2_72 = ["v2_70"]
v2_74 = ["v2_72"]
v2_76 = ["v2_74"]
v2_78 = ["v2_76"]
v2_80 = ["v2_78"]
v2_82 = ["v2_80"]
v2_84 = ["v2_82"]
v2_86 = ["v2_84"]

[lib]
name = "gio_sys"

[package]
build = "build.rs"
description = "FFI bindings to libgio-2.0"
keywords = ["gio", "ffi", "gtk-rs", "gnome"]
name = "gio-sys"

[package.authors]
workspace = true

[package.edition]
workspace = true

[package.exclude]
workspace = true

[package.homepage]
workspace = true

[package.license]
workspace = true

[package.repository]
workspace = true

[package.rust-version]
workspace = true

[package.version]
workspace = true

[package.metadata.system-deps.gio_2_0]
name = "gio-2.0"
version = "2.56"

[package.metadata.system-deps.gio_2_0.v2_58]
version = "2.58"

