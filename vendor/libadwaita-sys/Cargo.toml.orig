[package]
name = "libadwaita-sys"
version = "0.8.1"
authors = ["Bilal Elmoussaoui <bil.elmoussaoui@gmail.com>"]
edition = "2021"
build = "build.rs"
links = "libadwaita-1"
categories = ["api-bindings", "gui"]
keywords = ["gtk4-rs", "gnome", "adwaita", "GUI"]
description = "FFI bindings for libadwaita"
license = "MIT"
homepage = "https://world.pages.gitlab.gnome.org/Rust/libadwaita-rs/"
repository = "https://gitlab.gnome.org/World/Rust/libadwaita-rs"
documentation = "https://world.pages.gitlab.gnome.org/Rust/libadwaita-rs/stable/latest/docs/libadwaita_sys"

[package.metadata.system-deps.libadwaita_1]
name = "libadwaita-1"
version = "1"

[package.metadata.system-deps.libadwaita_1.v1_1]
version = "1.1"

[package.metadata.system-deps.libadwaita_1.v1_2]
version = "1.2"

[package.metadata.system-deps.libadwaita_1.v1_3]
version = "1.3"

[package.metadata.system-deps.libadwaita_1.v1_4]
version = "1.4"

[package.metadata.system-deps.libadwaita_1.v1_5]
version = "1.5"

[package.metadata.system-deps.libadwaita_1.v1_6]
version = "1.6"

[package.metadata.system-deps.libadwaita_1.v1_7]
version = "1.7"

[package.metadata.system-deps.libadwaita_1.v1_8]
version = "1.8"

[package.metadata.system-deps.libadwaita_1.v1_9]
version = "1.9"

[package.metadata.docs.rs]
features = []
rustc-args = ["--cfg", "docsrs"]
rustdoc-args = ["--cfg", "docsrs", "--generate-link-to-definition"]

[lib]
name = "libadwaita_sys"

[dependencies]
libc = "0.2"

[dependencies.gio-sys]
git = "https://github.com/gtk-rs/gtk-rs-core"
branch = "0.21"
version = "0.21"

[dependencies.glib-sys]
git = "https://github.com/gtk-rs/gtk-rs-core"
branch = "0.21"
version = "0.21"

[dependencies.gobject-sys]
git = "https://github.com/gtk-rs/gtk-rs-core"
branch = "0.21"
version = "0.21"

[dependencies.gtk-sys]
package = "gtk4-sys"
git = "https://github.com/gtk-rs/gtk4-rs"
branch = "0.10"
version = "0.10"

[dependencies.gdk-sys]
package = "gdk4-sys"
git = "https://github.com/gtk-rs/gtk4-rs"
branch = "0.10"
version = "0.10"

[dependencies.pango-sys]
git = "https://github.com/gtk-rs/gtk-rs-core"
branch = "0.21"
version = "0.21"

[build-dependencies]
system-deps = "7"

[dev-dependencies]
shell-words = "1.0.0"
tempfile = "3"

[features]
v1_1 = []
v1_2 = ["v1_1"]
v1_3 = ["v1_2"]
v1_4 = ["v1_3"]
v1_5 = ["v1_4"]
v1_6 = ["v1_5"]
v1_7 = ["v1_6"]
v1_8 = ["v1_7"]
v1_9 = ["v1_8"]
