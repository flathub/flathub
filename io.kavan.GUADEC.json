{
    "app-id" : "io.kavan.GUADEC",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "3.38",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command" : "guadec-app",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--talk-name=org.freedesktop.portal.*",
        "--talk-name=org.a11y.Bus"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/rust-stable/bin",
        "env" : {
            "CARGO_HOME" : "/run/build/guadec-app/cargo",
            "RUST_BACKTRACE" : "1",
            "RUST_LOG" : "guadec-app=debug"
        }
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "libhandy",
            "buildsystem" : "meson",
            "config-opts" : [
                "-Dintrospection=disabled",
                "-Dtests=false",
                "-Dexamples=false",
                "-Dvapi=false",
                "-Dglade_catalog=disabled"
            ],
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://gitlab.gnome.org/GNOME/libhandy.git",
                    "commit": "465c00f8f80c27330be494ed7c0ba2ffe26321c4",
                    "tag" : "1.0.2"
                }
            ]
        },
        {
            "name" : "guadec-app",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type": "archive",
                    "url" : "https://gitlab.gnome.org/kavanmevada/guadec-events/-/jobs/998397/artifacts/raw/guadec-events.tar.xz",
                    "sha256": "0f9989e098da3dfba0d6b2c655ee3ca7d7d45af6d8395989035dca70f47b2040"
                }
            ]
        }
    ]
}
