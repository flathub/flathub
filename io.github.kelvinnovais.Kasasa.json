{
  "id": "io.github.kelvinnovais.Kasasa",
  "runtime": "org.gnome.Platform",
  "runtime-version": "47",
  "sdk": "org.gnome.Sdk",
  "command": "kasasa",
  "finish-args": [
    "--share=ipc",
    "--socket=fallback-x11",
    "--device=dri",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/man",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig",
    "*.la",
    "*.a"
  ],
  "modules": [
    {
      "name": "libportal",
      "buildsystem": "meson",
      "builddir": true,
      "config-opts": [
        "-Dbackend-gtk3=disabled",
        "-Dbackend-gtk4=enabled",
        "-Dbackend-qt5=disabled",
        "-Dbackend-qt6=disabled",
        "-Dportal-tests=false",
        "-Dintrospection=true",
        "-Ddocs=false",
        "-Dtests=false",
        "-Dvapi=false"
      ],
      "sources": [
        {
          "type": "git",
          "url": "https://github.com/flatpak/libportal.git",
          "tag": "0.8.1"
        }
      ]
    },
    {
      "name": "kasasa",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "https://github.com/KelvinNovais/Kasasa",
          "tag": "v1.0.0",
          "commit": "f3e0c0d33d810ea96f0679d25dc76dff3827e0f5"
        }
      ]
    }
  ]
}
