{
    "name": "fftw3f",
    "config-opts": [
        "--enable-threads",
        "--enable-shared",
        "--disable-static",
        "--enable-float"
    ],
    "build-options": {
        "arch": {
            "x86_64": {
                "config-opts": [
                    "--enable-sse2",
                    "--enable-avx",
                    "--enable-avx-128-fma"
                ]
            },
            "aarch64": {
                "config-opts": [
                    "--enable-neon"
                ]
            }
        }
    },
    "sources": [
        {
            "type": "archive",
            "url": "https://www.fftw.org/fftw-3.3.10.tar.gz",
            "sha256": "56c932549852cddcfafdab3820b0200c7742675be92179e59e6215b340e26467"
        }
    ],
    "cleanup": [
        "/bin",
        "/include",
        "/lib/cmake",
        "/lib/pkgconfig",
        "/share/man",
        "*.la",
        "*.so"
    ]
}
