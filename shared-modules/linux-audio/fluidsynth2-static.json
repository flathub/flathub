{
  "name": "fluidsynth",
  "buildsystem": "cmake-ninja",
  "build-options": {
    "env": {
      "CFLAGS": "-fPIC",
      "CXXFLAGS": "-fPIC"
    }
  },
  "config-opts": [
    "-DLIB_SUFFIX=",
    "-DBUILD_SHARED_LIBS=OFF"
  ],
  "cleanup": [
    "/bin",
    "/include",
    "/lib/pkgconfig",
    "/share/man",
    "*.so",
    "*.a"
  ],
  "sources": [
    {
      "type": "archive",
      "url": "https://github.com/FluidSynth/fluidsynth/archive/v2.2.5.tar.gz",
      "sha256": "9037e703617f91c4c36039a5059e0f624164799d856af715bcd8a23c07ba03b8"
    }
  ]
}
