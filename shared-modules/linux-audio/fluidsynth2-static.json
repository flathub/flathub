{
  "name": "fluidsynth",
  "buildsystem": "cmake-ninja",
  "build-options": {
    "env": {
      "CFLAGS": "-fPIC",
      "CXXFLAGS": "-fPIC"
    }
  },
  "config-opts": [
    "-DLIB_SUFFIX=",
    "-DBUILD_SHARED_LIBS=OFF"
