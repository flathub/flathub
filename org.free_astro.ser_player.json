{
	"id": "org.free_astro.ser_player",
	"runtime": "org.kde.Platform",
	"runtime-version": "6.8",
	"sdk": "org.kde.Sdk",
	"command": "ser-player",
	"cleanup": [
		"/include",
		"*.a",
		"*.la",
		"*.cmake"
	],
	"finish-args":
	[
		"--share=ipc",
		"--socket=fallback-x11",
		"--socket=wayland",
		"--device=dri"
	],
	"modules": [
		{
			"name": "ser-player",
			"buildsystem": "qmake",
			"config-opts": ["CONFIG+=release", "BUILD_FOR_REPOSITORY="],
			"sources": [
				{
					"type": "git",
					"url": "https://github.com/lock042/ser-player.git",
					"commit": "45f7b04398099777c3f0741dee14924abd045dc5"
				}
			]
		}
	]
}
