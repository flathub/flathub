{
    "name": "qt5-qtwebkit",
    "buildsystem": "cmake",
    "builddir": true,
    "config-opts": [
        "-DCMAKE_BUILD_TYPE=Release",
        "-DCMAKE_INSTALL_LIBDIR=lib",
        "-DPORT=Qt"
    ],
    "sources": [
        {
            "type": "git",
            "url": "git://code.qt.io/qt/qtwebkit.git",
            "branch": "5.212"
        }
    ],
    "post-install": [
        "mv '/app/mkspecs' '/app/lib';"
    ],
    "cleanup-platform": [
        "/mkspecs"
    ]
}
