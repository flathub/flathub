{
    "name": "qt5-qtwebkit",
    "buildsystem": "cmake",
    "builddir": true,
    "config-opts": [
        "-DCMAKE_BUILD_TYPE=Release",
        "-DCMAKE_INSTALL_LIBDIR=lib",
        "-DCMAKE_VERBOSE_MAKEFILE:BOOL=ON",
        "-DPORT=Qt"
    ],
    "build-options": {
        "arch": {
            "i386": {
                "cflags": "-O2 -g1 -Wno-deprecated-declarations",
                "cxxflags": "-O2 -g1 -Wno-deprecated-declarations -fpermissive"
            },
            "arm": {
                "cflags": "-O2 -g1 -Wno-deprecated-declarations",
                "cxxflags": "-O2 -g1 -Wno-deprecated-declarations -fpermissive"
            }
        }
    },
    "sources": [
        {
