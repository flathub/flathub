{
  "id": "hu.chatenium.chtnoladw",
  "runtime": "org.gnome.Platform",
  "runtime-version": "49",
  "sdk": "org.gnome.Sdk",
  "command": "chateniumadw",
  "finish-args": [
    "--share=network",
    "--share=ipc",
    "--socket=fallback-x11",
    "--device=dri",
    "--socket=wayland",
    "--talk-name=org.freedesktop.secrets"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/man",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig",
    "*.la",
    "*.a"
  ],
  "modules": [
    "pypi-dependencies.json",
    {
      "name": "chateniumadw",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "https://github.com/chatenium/Chatenium-On-Linux-Adwaita",
          "tag": "0.1.0"
        }
      ]
    }
  ]
}
