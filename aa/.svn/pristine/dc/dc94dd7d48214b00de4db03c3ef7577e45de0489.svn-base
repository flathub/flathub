# separately from the GPL2 licensed source code.
#

AC_PREREQ([2.68])
AC_INIT([alienarena], [7.71.4])
AC_CONFIG_SRCDIR([pics/colormap.pcx])
AC_CONFIG_AUX_DIR([build-aux])

AM_INIT_AUTOMAKE( 1.11.1 foreign )

AM_MAINTAINER_MODE

AC_CONFIG_FILES([Makefile
				botinfo/Makefile
				env/Makefile
				gfx/Makefile
				levelshots/Makefile
				maps/Makefile
				models/Makefile
				particles/Makefile
				pics/Makefile
				players/Makefile
				scripts/Makefile
				sound/Makefile
				textures/Makefile
				vehicles/Makefile])

AC_OUTPUT
