{
    "appId": "com.CalculaPro.app",
    "productName": "Calcula Pro",
    "extraMetadata": {
        "name": "Calcula Pro"
    },
    "copyright": "Copyright Â© 2024 Alejandro Penate-Diaz",
    "buildVersion": "1.8.4",
    "compression": "maximum",
    "directories": {
        "output": "../../../bin/Desktop"
    },
    "extraResources": [
        {
            "from": "./bin",
            "to": "bin",
            "filter": [
                "**/*"
            ]
        }
    ],
    "linux": {
        "category": "Productivity",
        "target": [
            {
                "target": "flatpak",
                "arch": [
                    "x64",
                    "arm64"
                ]
            }
        ]
    },
    "files": [
        {
            "from": "./ElectronHostHook/node_modules",
            "to": "ElectronHostHook/node_modules",
            "filter": [
                "**/*"
            ]
        },
        "**/*"
    ]
}
