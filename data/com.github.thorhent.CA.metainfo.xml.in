<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
  <id>com.github.thorhent.CA</id>
  <metadata_license>CC0-1.0</metadata_license>
  <project_license>MIT</project_license>

  <name>Clinicalayudante</name>
  <summary>Aplicación de ayuda a la practica médica.</summary>
  <description>
    <p>Aplicación que recibe una lista de síntomas o signos como entrada, los procesa
      y retorna una lista de posibles enfermedades en orden de mayor para menor probabilidad. </p>
  </description>

  <developer id="tld.vendor">
    <name>Taylan Branco Meurer</name>
  </developer>

  <update_contact>tbmeurer@gmail.com</update_contact>

  <!-- Requered: Should be a link to the upstream homepage for the component -->
  <url type="homepage">https://taylan.pythonanywhere.com/</url>
  <!-- Recommended: It is highly recommended for open-source projects to display the source code repository -->
  <url type="vcs-browser">https://github.com/thorhent/CA</url>
  <!-- Should point to the software's bug tracking system, for users to report new bugs -->
  <url type="bugtracker">https://github.com/thorhent/CA/issues</url>


  <url type="help">https://github.com/thorhent/CA/wiki</url>
  <!-- URLs of this type should point to a webpage showing information on how to donate to the described software project -->

  <translation type="gettext">clinicalayudante</translation>
  <!-- All graphical applications having a desktop file must have this tag in the MetaInfo.
     If this is present, appstreamcli compose will pull icons, keywords and categories from the desktop file. -->
  <launchable type="desktop-id">com.github.thorhent.CA.desktop</launchable>
  <!-- Use the OARS website (https://hughsie.github.io/oars/generate.html) to generate these and make sure to use oars-1.1 -->
  <content_rating type="oars-1.1" />

  <!-- Applications should set a brand color in both light and dark variants like so -->
  <branding>
    <color type="primary" scheme_preference="light">#ff00ff</color>
    <color type="primary" scheme_preference="dark">#993d3d</color>
  </branding>

  <screenshots>
    <screenshot type="default">
      <image>https://github.com/thorhent/CA/blob/master/screenshot/1.png</image>
      <caption>Ventana inicial</caption>
    </screenshot>
    <screenshot>
      <image>https://github.com/thorhent/CA/blob/master/screenshot/2.png</image>
      <caption>Enfermedades posibles</caption>
    </screenshot>
  </screenshots>

  <releases>
    <release version="1.2.25" date="2025-02-20">
      <!--<url type="details">https://example.org/changelog.html#version_1.0.1</url>-->
      <description translate="no">
        <p>Base de datos en desarrollo</p>
        <ul>
          <li>140 enfermedades registradas</li>
        </ul>
      </description>
    </release>
  </releases>

</component>
