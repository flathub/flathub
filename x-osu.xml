<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
    <mime-type type="application/x-osu-beatmap">
	<sub-class-of type="text/plain"/>
        <comment>osu! beatmap</comment>
        <comment xml:lang="hk">osu! 譜面</comment>
        <comment xml:lang="zh">osu! 谱面</comment>
        <comment xml:lang="ru">Карта osu!</comment>
        <icon name="sh.ppy.osu.x-osu-beatmap"/>
        <glob pattern="*.osu"/>
    </mime-type>
    <mime-type type="application/x-osu-beatmap-archive">
        <sub-class-of type="application/zip"/>
        <comment>osu! beatmap archive</comment>
        <comment xml:lang="ru">Архив карты osu!</comment>
        <icon name="sh.ppy.osu.x-osu-beatmap"/>
        <glob pattern="*.osz"/>
    </mime-type>
    <mime-type type="application/x-osu-skin-archive">
        <sub-class-of type="application/zip"/>
        <comment>osu! skin archive</comment>
        <comment xml:lang="ru">Архив скина osu!</comment>
        <icon name="sh.ppy.osu.x-osu-beatmap"/>
        <glob pattern="*.osk"/>
    </mime-type>
    <mime-type type="application/x-osu-storyboard">
	<sub-class-of type="text/plain"/>
        <comment>osu! storyboard</comment>
        <comment xml:lang="ru">Дизайн заднего плана osu!</comment>
        <icon name="sh.ppy.osu.x-osu-beatmap"/>
        <glob pattern="*.osb"/>
    </mime-type>
    <mime-type type="application/x-osu-replay">
	<sub-class-of type="application/octet-stream"/>
        <comment>osu! replay</comment>
        <comment xml:lang="ru">Реплей osu!</comment>
        <icon name="sh.ppy.osu.x-osu-beatmap"/>
        <glob pattern="*.osr"/>
    </mime-type>
</mime-info>
