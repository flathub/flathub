{
  "app-id": "org.duckdns.bugfreeblog.mldonkeynext",
  "runtime": "org.kde.Platform",
  "runtime-version": "6.9",
  "sdk": "org.kde.Sdk",
  "base": "io.qt.qtwebengine.BaseApp",
